     ]
    }
   ],
   "source": [
    "Scenestree2_FS = DecisionTreeClassifier(max_depth =6, random_state=0)\n",
    "Scenestree2_FS.fit(X_train_FS, y_train_FS)\n",
    "y_pred2_FS = Scenestree2_FS.predict(X_test_FS)\n",
    "print(\"Accuracy on training set: {:.3f}\".format(Scenestree2_FS.score(X_train_FS, y_train_FS)))\n",
    "print(\"Accuracy on test set: {:.3f}\".format(Scenestree2_FS.score(X_test_FS, y_test_FS)))"
